{
    "name": "kunstmaan/seo-bundle",
    "type": "symfony-bundle",
    "description": "Annotating content with metadata for social sharing and seo purposes cannot be overlooked nowadays. The KunstmaanSeoBundle contains default editing functionality for OpenGraph data, meta descriptions, keywords and titles and Metriweb tags. Because the metatagging and tracking options are always changing, a free field to add custom header information is provided as well.",
    "keywords": [ "kunstmaan", "seo", "cms", "opengraph", "meta"],
    "homepage": "https://github.com/Kunstmaan/KunstmaanSeoBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kunstmaan Team",
            "homepage": "http://www.kunstmaan.be/",
            "email": "support@kunstmaan.be"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": ">=2.2,<2.3",
        "gedmo/doctrine-extensions": "*",
        "kunstmaan/admin-bundle": "2.2.*",
        "kunstmaan/media-bundle": "2.2.*",
        "kunstmaan/utilities-bundle": "2.2.*"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "2.1.*@dev",
        "stof/doctrine-extensions-bundle": "dev-master",
        "white-october/pagerfanta-bundle": "dev-master",
        "pagerfanta/pagerfanta": "dev-master",
        "friendsofsymfony/user-bundle": "2.0.*@dev"
    },
    "autoload": {
        "psr-0": { "Kunstmaan\\SeoBundle": "" }
    },
    "target-dir": "Kunstmaan/SeoBundle"
}
